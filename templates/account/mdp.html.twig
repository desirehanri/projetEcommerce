{% extends 'base.html.twig' %}

{% block title %}Password page{% endblock %}

{% block content %}
    <h1>Change your password</h1>
    <a href ="{{path('app_account')}}">Back to your account</a><br>
    {% if notifS %}
        <div class="alert alert-success">{{ notifS }}</div>
    {% elseif notifE %}
        <div class="alert alert-danger" >{{ notifE }}</div>
    {% endif %}
    <hr>
        {{form(form)}}
    <hr>
{% endblock %}
